            AddHandler KeyDown, AddressOf OnKeyDown

            _menuPool = New MenuPool()
            CreateBuyMenu()
            CreateExitMenu()
            CreateGarageMenu()

            AddHandler BuyMenu.OnMenuClose, AddressOf MenuCloseHandler
            AddHandler ExitMenu.OnMenuClose, AddressOf MenuCloseHandler
